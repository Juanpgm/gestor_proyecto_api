# Configuración para entorno LOCAL
# Configuración de Base de datos PostgreSQL Local
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dev
DB_USER=postgres  
DB_PASSWORD=root
DB_SCHEMA=public

# Configuración de PostGIS
POSTGIS_VERSION=3.3

# Configuración del logging
LOG_LEVEL=DEBUG
LOG_FILE=logs/app.log

# Configuración de la aplicación
APP_NAME=API Gestor de Proyectos - Local
APP_VERSION=1.0.0
APP_ENV=development

# Configuración de Conexión
CONNECTION_TIMEOUT=30
POOL_SIZE=5 
MAX_OVERFLOW=10

# Para desarrollo local, usar configuración individual
# DATABASE_URL se construirá automáticamente a partir de los parámetros DB_*